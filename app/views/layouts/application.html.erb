<!DOCTYPE html>
<html>
  <head>
    <title>RoRlog</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag '/assets/css/materialize.min.css', media: 'all' %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= javascript_include_tag '/assets/js/materialize.min.js' %>
    <%= yield %>
  </body>
</html>
